{
  "name": "@mixnjuice/sequelize-mock",
  "version": "0.11.0",
  "description": "A simple mock interface specifically for testing code relying on Sequelize models",
  "main": "src/index.js",
  "scripts": {
    "doc-gen": "node scripts/doc-gen.js",
    "test": "nyc --reporter=text --reporter=html mocha",
    "test-report": "nyc report --reporter=html",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "preversion": "npm test",
    "version": "node scripts/version.js && git add changelog.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mixnjuice/sequelize-mock.git"
  },
  "keywords": [
    "sequelize",
    "mocking",
    "test",
    "testing"
  ],
  "author": "Blink UX",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mixnjuice/sequelize-mock/issues"
  },
  "homepage": "https://github.com/mixnjuice/sequelize-mock#readme",
  "dependencies": {
    "bluebird": "^3.5.5",
    "inflection": "^1.12.0",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "coveralls": "^3.0.6",
    "dox": "^0.9.0",
    "glob": "^7.1.4",
    "istanbul": "^0.4.5",
    "mocha": "^6.2.0",
    "moment": "^2.24.0",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "proxyquire": "^2.1.3",
    "should": "^13.2.3"
  },
  "nyc": {
    "exclude": [
      "src/index.js"
    ],
    "include": [
      "src/**/*.js"
    ],
    "all": true
  }
}
